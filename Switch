public class Switch {
    public static void main(String[] args) {
        String day = "Monday";

        switch (day) {
            case "Friday" :
                System.out.println("It's Friday");
                break;
            case "Thursday":
                System.out.println("It's Thursday");
                break;
            case  "Wednesday" :
                System.out.println("It's Wednesday");
                break;
            case "Monday" :
                System.out.println("It's Monday");
        }
    }
}
