public class Expressions {
    public static void main(String[] args) {
        /*
         int dog = 10;
        dog = dog % 3; // = 0;
        dog = dog % 2; // = 1;

        dog--;
        dog++;
         */
        double dog = 10;
        dog = (double) dog / 3;
        System.out.println(dog);
    }
}
