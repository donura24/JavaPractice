public class ForLoop {
    public static void main(String[] args) {

        for (int i = 0; i <= 12; i++) {
            System.out.print(" "+i);
        }
        System.out.print(" We Graduated!!");
        System.out.println();
        for (int j = 12; j >= 1;) {
            j-=2;

            System.out.print(" "+j);

        }
    }
}
